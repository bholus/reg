    <script language="javascript" type="text/javascript">
        //Note  We have to add a script file in Masterpage/head of this page to run this java script
        function PreviewImg(imgFile, id) {

            var newPreview = document.getElementById(id);
            newPreview.src = imgFile.value;
            newPreview.filters.item("DXImageTransform.Microsoft.AlphaImageLoader").src = imgFile.value;
            newPreview.style.width = "100px";
            newPreview.style.height = "100px";
        }
    </script>

