    function CaptureMantra() {

        discoverAvdm();
        

        var t = document.getElementById('MainContent_hdnMethodCapture').value;
            var url = t;
           // alert(url);
            //alert(finalUrl);
            //alert(MethodCapture);

            var DrpVal = document.getElementById('MainContent_DropDownList1').value;
            if (DrpVal == "1")
                var PIDOPTS = '<PidOptions ver=\"1.0\">' + '<Opts fCount=\"1\" fType=\"0\" iCount=\"0\" iType=\"\" pCount=\"0\" pType=\"\" format=\"0\" pidVer=\"2.0\" timeout=\"10000\" otp=\"\" env=\"P\" wadh=\"\" posh=\"\"/>' + '</PidOptions>';
            else if (DrpVal == "2")
                var PIDOPTS = '<PidOptions ver=\"1.0\">' + '<Opts fCount=\"1\" fType=\"0\" iCount=\"\" iType=\"\" pCount=\"\" pType=\"\" format=\"0\" pidVer=\"2.0\" timeout=\"10000\" otp=\"\" wadh=\"\" posh=\"\"/>' + '</PidOptions>';
            else if (DrpVal == "3")
                var PIDOPTS = '<PidOptions ver=\"1.0\">' + '<Opts fCount=\"0\" fType=\"0\" iCount=\"1\" iType=\"\" pCount=\"0\" pType=\"\" format=\"0\" pidVer=\"2.0\" timeout=\"50000\" otp=\"\" env=\"P\" wadh=\"\" posh=\"\"/>' + '</PidOptions>';
            /*format=\"0\"     --> XML
         format=\"1\"     --> Protobuf
         */




            var xhr;
            var ua = window.navigator.userAgent;
            var msie = ua.indexOf("MSIE ");
            if (msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./)) // If Internet Explorer, return version number
            {
                //IE browser
                xhr = new ActiveXObject("Microsoft.XMLHTTP");
            } else {
                //other browser
                xhr = new XMLHttpRequest();
            }
            xhr.open('CAPTURE', url, true);
            xhr.setRequestHeader("Content-Type", "text/xml");
            xhr.setRequestHeader("Accept", "text/xml");

            xhr.onreadystatechange = function () {
                //if(xhr.readyState == 1 && count == 0){
                //	fakeCall();
                //}
                if (xhr.readyState == 4) {
                    var status = xhr.status;

                    if (status == 200) {
                        debugger;
                        //var dat = document.getElementById('MainContent_lblData').innerText;
                        // alert(dat);
                        // alert(xhr.responseText);
                        document.getElementById('MainContent_hndData').value = xhr.responseText;
                        var result = xhr.responseText;
                        if (result.toString().length > 500) {
                            document.getElementById('MainContent_Label2').innerText = "Capture Succcessful";
                            document.getElementById('MainContent_Image3').style.visibility = "visible";
                           document.getElementById('MainContent_Image2').style.visibility = "hidden";
                           document.getElementById('MainContent_Image5').style.visibility = "hidden";
                          
                      }
                        else {
                            if (DrpVal == "3")
                                document.getElementById('MainContent_Label2').innerText = "Please Insert your eyes Properly..!!!!!";
                            else 
                                document.getElementById('MainContent_Label2').innerText = "Please Insert your thumb properly....!!!!!";
                      }
                        //if(xhr.responseText.indexOf("errCode=\"100\"") != -1){
                        //alert(xhr.responseText);
                        //fakeCall();
                        //Capture();
                        //}else{
                        //    alert(xhr.responseText);
                        //    console.log(xhr.response);
                        //}
                  } else {

                      console.log(xhr.response);
                  }
              }
            };
            // document.forms[0].appendChild(xhr.send(PIDOPTS));
          xhr.send(PIDOPTS);
      }
    </script>

    <script language="javascript" type="text/javascript">
        function ShowHide(Id, img) {
            var div = document.getElementById(Id);
            var img = document.getElementById(img);
            if (div.style.display == "none") {
                div.style.display = "";
                img.src = "../Images/RemoveIcon.png"

            }
            else {
                div.style.display = "none";
                img.src = "../Images/AddIcon.png"
            }
        }      

    </script>
    <script language="JavaScript" type="text/javascript">
        function onlyNumbers(evt) {
            var e = event || evt;
            var charCode = e.which || e.keyCode;
            if (charCode > 31 && (charCode < 48 || charCode > 57)) {
                alert('Enter Numbers Only');
                return false;
            }
            return true;
        } 
    </script>
