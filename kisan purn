    <script type="text/javascript">

        var finalUrl = "";
        var MethodInfo = "";
        var MethodCapture = "";

              //Mantra Discover Function
          function discoverAvdmFirstNode(PortNo) {
             // alert(PortNo);
              //var primaryUrl = "http://127.0.0.1:";

              var primaryUrl = "http://127.0.0.1:";

              try {
                  var protocol = window.location.href;
                  if (protocol.indexOf("https") >= 0) {
                      primaryUrl = "https://127.0.0.1:";
                  }
              } catch (e)
              { }
              url = "";
              var verb = "RDSERVICE";
              var err = "";
              var res;
              $.support.cors = true;
              var httpStaus = false;
              var jsonstr = "";
              var data = new Object();
              var obj = new Object();

              //alert(primaryUrl);

              $.ajax({
                  type: "RDSERVICE",
                  async: false,
                  crossDomain: true,
                  url: primaryUrl + PortNo,
                  contentType: "text/xml; charset=utf-8",
                  processData: false,
                  cache: false,
                  async: false,
                  crossDomain: true,
                  success: function (data) {
                      httpStaus = true;
                      res = { httpStaus: httpStaus, data: data };
                      finalUrl = primaryUrl + PortNo.toString();
                      //alert(data);
                      //debugger;
                      var $doc = $.parseXML(data);

                      //alert($($doc).find('Interface').eq(1).attr('path'));


                      if ($($doc).find('Interface').eq(0).attr('path') == "/rd/capture") {
                          MethodCapture = $($doc).find('Interface').eq(0).attr('path');
                      }
                      if ($($doc).find('Interface').eq(1).attr('path') == "/rd/capture") {
                          MethodCapture = $($doc).find('Interface').eq(1).attr('path');
                      }

                      if ($($doc).find('Interface').eq(0).attr('path') == "/rd/info") {
                          MethodInfo = $($doc).find('Interface').eq(0).attr('path');
                      }
                      if ($($doc).find('Interface').eq(1).attr('path') == "/rd/info") {
                          MethodInfo = $($doc).find('Interface').eq(1).attr('path');
                      }
                    //  alert("RDSERVICE 1 Discover Successfully");

                      //alert(finalUrl + MethodCapture);
                  },
                  error: function (jqXHR, ajaxOptions, thrownError) {

                      //alert(thrownError);
                      res = { httpStaus: httpStaus, err: getHttpError(jqXHR) };
                  },
              });

              return res;
          }

          function discoverAvdm() {



              var DrpVal = document.getElementById('MainContent_DropDownList1').value;



           


            var SuccessFlag = 0;
            var primaryUrl = "http://127.0.0.1:";

            try {
                var protocol = window.location.href;
                if (protocol.indexOf("https") >= 0) {
                    primaryUrl = "https://127.0.0.1:";
                }
            } catch (e)
            { }

              //alert(primaryUrl);
            if (DrpVal == '2') {
                primaryUrl = "http://127.0.0.1:"
            }
             
            debugger;
            url = "";
            //alert("Please wait while discovering port from 11100 to 11120.\nThis will take some time.");
            for (var i = 11100; i <= 11120; i++) {

                var verb = "RDSERVICE";
                var err = "";
                SuccessFlag = 0;
                var res;
                $.support.cors = true;
                var httpStaus = false;
                var jsonstr = "";
                var data = new Object();
                var obj = new Object();

             



                $.ajax({

                    type: "RDSERVICE",
                    async: false,
                    crossDomain: true,
                    url: primaryUrl + i.toString(),
                    contentType: "text/xml; charset=utf-8",
                    processData: false,
                    cache: false,
                    crossDomain: true,

                    success: function (data) {

                        httpStaus = true;
                        res = { httpStaus: httpStaus, data: data };
                        //alert(data);
                        finalUrl = primaryUrl + i.toString();
                        var $doc = $.parseXML(data);
                        var CmbData1 = $($doc).find('RDService').attr('status');
                        var CmbData2 = $($doc).find('RDService').attr('info');

                        debugger;
                        if (RegExp('\\b' + 'Mantra' + '\\b').test(CmbData2) == true && DrpVal == '1') {

                            if ($($doc).find('Interface').eq(0).attr('path') == "/rd/capture") {
                                MethodCapture = $($doc).find('Interface').eq(0).attr('path');
                            }
                            if ($($doc).find('Interface').eq(1).attr('path') == "/rd/capture") {
                                MethodCapture = $($doc).find('Interface').eq(1).attr('path');
                            }
                            if ($($doc).find('Interface').eq(0).attr('path') == "/rd/info") {
                                MethodInfo = $($doc).find('Interface').eq(0).attr('path');
                            }
                            if ($($doc).find('Interface').eq(1).attr('path') == "/rd/info") {
                                MethodInfo = $($doc).find('Interface').eq(1).attr('path');
                            }


                            SuccessFlag = 1;
                           // alert("RDSERVICE Discover Successfully");

                            //debugger;
                            var t = finalUrl + MethodCapture;
                            document.getElementById('MainContent_hdnMethodCapture').value = t;



                            // alert(t);

                            //alert(finalUrl + MethodCapture);
                            return;

                        }
                        else if (DrpVal == '2') {

                            if ($($doc).find('Interface').eq(0).attr('path') == "/capture") {
                                MethodCapture = $($doc).find('Interface').eq(0).attr('path');
                            }
                            if ($($doc).find('Interface').eq(1).attr('path') == "/capture") {
                                MethodCapture = $($doc).find('Interface').eq(1).attr('path');
                            }
                            if ($($doc).find('Interface').eq(0).attr('path') == "/info") {
                                MethodInfo = $($doc).find('Interface').eq(0).attr('path');
                            }
                            if ($($doc).find('Interface').eq(1).attr('path') == "/info") {
                                MethodInfo = $($doc).find('Interface').eq(1).attr('path');
                            }


                            SuccessFlag = 1;
                            // alert("RDSERVICE Discover Successfully.... morpho");

                            //debugger;
                            var t = finalUrl + '/capture';
                            document.getElementById('MainContent_hdnMethodCapture').value = t;


                            // alert(t);

                            //alert(finalUrl + MethodCapture);
                            return;

                        }
                        if (RegExp('\\b' + 'Mantra IRIS' + '\\b').test(CmbData2) == true && DrpVal == '3') {

                            if ($($doc).find('Interface').eq(0).attr('path') == "/rd/capture") {
                                MethodCapture = $($doc).find('Interface').eq(0).attr('path');
                            }
                            if ($($doc).find('Interface').eq(1).attr('path') == "/rd/capture") {
                                MethodCapture = $($doc).find('Interface').eq(1).attr('path');
                            }
                            if ($($doc).find('Interface').eq(0).attr('path') == "/rd/info") {
                                MethodInfo = $($doc).find('Interface').eq(0).attr('path');
                            }
                            if ($($doc).find('Interface').eq(1).attr('path') == "/rd/info") {
                                MethodInfo = $($doc).find('Interface').eq(1).attr('path');
                            }


                            SuccessFlag = 1;
                           // alert("RDSERVICE Discover IRIS Successfully");

                            //debugger;
                            var t = finalUrl + MethodCapture;
                            document.getElementById('MainContent_hdnMethodCapture').value = t;



                                // alert(t);

                                //alert(finalUrl + MethodCapture);
                            return;

                        }
                        else {

                        }

                        //  alert(CmbData1);
                        //  alert(CmbData2);

                    },
                    error: function (jqXHR, ajaxOptions, thrownError) {
                        if (i == "8005" && OldPort == true) {
                            OldPort = false;
                            i = "11099";
                        }

                        //alert(thrownError);

                        //res = { httpStaus: httpStaus, err: getHttpError(jqXHR) };
                    },

                });



            if (SuccessFlag == 1) {
                break;
            }

                //$("#ddlAVDM").val("0");

        }

        if (SuccessFlag == 0) {
            //alert("Connection failed Please try again.");
        }



            //$('#txtDeviceInfo').val(DataXML);



        return res;
    }
